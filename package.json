{
  "name": "sensitive-words",
  "version": "1.0.0",
  "description": "Filter out a list of sensitive words",
  "main": "build/index.js",
  "scripts": {
    "build": "babel src -d build",
    "dev": "watch 'npm run build' src",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "prepublish": "npm run build"
  },
  "babel": {
    "presets": [
      "latest"
    ],
    "ignore": [
      "**/*.test.js",
      "**/*.spec.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Waserman/sensitive-words.git"
  },
  "keywords": [
    "filter",
    "filtering",
    "sensitive",
    "words"
  ],
  "author": "Avi Waserman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Waserman/sensitive-words/issues"
  },
  "homepage": "https://github.com/Waserman/sensitive-words#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-latest": "^6.24.1",
    "jest": "^24.5.0"
  },
  "devDependencies": {
    "watch": "^1.0.2"
  }
}
